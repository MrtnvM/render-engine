{
  "name": "render-cli",
  "version": "1.0.1",
  "description": "CLI tool for Render Engine",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "render": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@render-engine/admin-sdk": "workspace:*",
    "@supabase/supabase-js": "^2.57.4",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "commander": "^12.0.0",
    "dotenv": "^17.2.2",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.13",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "react": "^19.1.1",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "cli",
    "render",
    "engine",
    "command-line"
  ],
  "author": "",
  "license": "MIT"
}
